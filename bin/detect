#!/bin/bash

echo $ENFORCE_APP_NAMING_PATTERN

# Check if ENFORCE_APP_NAMING_PATTERN is set
if [[ -z $ENFORCE_APP_NAMING_PATTERN ]]; then
    # The environment variable is not set, so we exit successfully
    echo "ENFORCE_APP_NAMING_PATTERN not set."
    exit 1
else
    echo "ENFORCE_APP_NAMING_PATTERN is set to $ENFORCE_APP_NAMING_PATTERN"
    exit 0
fi
